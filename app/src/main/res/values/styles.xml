<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/roxo</item>
        <item name="colorPrimaryDark">@color/roxo</item>
        <item name="colorAccent">@color/roxo</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="AppTheme" />

    <style name="AppTheme.PopupOverlay" parent="AppTheme.AppBarOverlay" />

</resources>
